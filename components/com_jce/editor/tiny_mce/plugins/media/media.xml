<?xml version="1.0" ?>
<extension version="3.4" type="plugin" group="jce" method="upgrade">
    <name>WF_MEDIA_TITLE</name>
    <version>@@version@@</version>
    <creationDate>@@date@@</creationDate>
    <author>Ryan Demmer</author>
    <authorEmail>info@joomlacontenteditor.net</authorEmail>
    <authorUrl>https://www.joomlacontenteditor.net</authorUrl>
    <copyright>@@copyright@@</copyright>
    <license>@@licence@@</license>
    <description>WF_MEDIA_DESC</description>
    <icon></icon>
    <fields name="media">
        <fieldset name="config">
            <field name="strict" type="yesno" default="1" label="WF_MEDIA_PARAM_STRICT" description="WF_MEDIA_PARAM_STRICT_DESC">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
            <field name="iframes" type="yesno" default="0" label="WF_MEDIA_PARAM_IFRAMES" description="WF_MEDIA_PARAM_IFRAMES_DESC">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
            <field name="audio" type="yesno" default="1" label="WF_MEDIA_PARAM_AUDIO" description="WF_MEDIA_PARAM_AUDIO_DESC">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
            <field name="video" type="yesno" default="1" label="WF_MEDIA_PARAM_VIDEO" description="WF_MEDIA_PARAM_VIDEO_DESC">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
            <field name="object" type="yesno" default="1" label="WF_MEDIA_PARAM_OBJECT" description="WF_MEDIA_PARAM_OBJECT_DESC">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
            <field name="embed" type="yesno" default="1" label="WF_MEDIA_PARAM_EMBED" description="WF_MEDIA_PARAM_EMBED_DESC">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>

            <field name="version_flash" type="text" size="50" default="10,1,53,64" label="WF_MEDIA_VERSION_FLASH" />
            <field name="version_windowsmedia" type="text" size="50" default="10,00,00,3646" label="WF_MEDIA_VERSION_WINDOWSMEDIA" />
            <field name="version_quicktime" type="text" size="50" default="7,3,0,0" label="WF_MEDIA_VERSION_QUICKTIME" />
            <field name="version_java" type="text" size="50" default="1,5,0,0" label="WF_MEDIA_VERSION_JAVA" />
            <field name="version_shockwave" type="text" size="50" default="10,2,0,023" label="WF_MEDIA_VERSION_SHOCKWAVE" />
        </fieldset>
    </fields>
    <languages></languages>
</extension>
